{
  "name": "@multi-bu/theme-engine",
  "version": "0.1.0",
  "private": true,
  "description": "Theme compilation engine for multi-BU design system",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.js",
      "types": "./dist/node.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/node.ts --format cjs,esm --dts --external @mui/material --external @mui/system --external zod",
    "dev": "tsup src/index.ts src/node.ts --format cjs,esm --dts --watch --external @mui/material --external @mui/system --external zod",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mui/material": "^5.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
